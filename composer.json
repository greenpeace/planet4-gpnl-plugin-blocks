{
  "name": "greenpeace/planet4-gpnl-plugin-blocks",
  "description": "This repository contains the WordPress plugin that provides the blocks to be used with Shortcake UI plugin.",
  "type": "wordpress-plugin",
  "version": "2.12.0",
  "license": "GPL-3.0+",
  "authors": [
    {
      "name": "Greenpeace Netherlands",
      "email": "planet4-group@greenpeace.org"
    },
		{
			"name": "Oscar Keur",
			"email": "okeur@greenpeace.org"
		},
		{
			"name": "Dirk Faber",
			"email": "dfaber@greenpeace.org"
		}
  ],
  "require-dev": {
    "squizlabs/php_codesniffer": "3.4.*",
    "wp-coding-standards/wpcs": "2.1.*",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.4.0"
  },
  "require": {
		"ext-curl": "*",
		"ext-openssl": "*"
	},
  "autoload": {
    "psr-4": {
      "P4NLBKS\\": "classes/",
      "P4NLBKS\\Controllers\\": "classes/controller/",
      "P4NLBKS\\Controllers\\Blocks\\": "classes/controller/blocks",
      "P4NLBKS\\Controllers\\Menu\\": "classes/controller/menu"
    }
  }
}
